{
  "version": 1,
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": [
              "apps/api/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/api/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/api"
          ],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "api-interfaces": {
      "root": "libs/api-interfaces",
      "sourceRoot": "libs/api-interfaces/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/api-interfaces/**/*.ts"
            ]
          }
        }
      },
      "tags": []
    },
    "it-security-investment-tool": {
      "projectType": "application",
      "root": "apps/it-security-investment-tool",
      "sourceRoot": "apps/it-security-investment-tool/src",
      "prefix": "it",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/it-security-investment-tool",
            "index": "apps/it-security-investment-tool/src/index.html",
            "main": "apps/it-security-investment-tool/src/main.ts",
            "polyfills": "apps/it-security-investment-tool/src/polyfills.ts",
            "tsConfig": "apps/it-security-investment-tool/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/it-security-investment-tool/src/favicon.ico",
              "apps/it-security-investment-tool/src/assets"
            ],
            "styles": [
              "apps/it-security-investment-tool/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2mb",
                  "maximumError": "2mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/it-security-investment-tool/src/environments/environment.ts",
                  "with": "apps/it-security-investment-tool/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "it-security-investment-tool:build:production"
            },
            "development": {
              "browserTarget": "it-security-investment-tool:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/it-security-investment-tool/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "it-security-investment-tool:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/it-security-investment-tool/src/**/*.ts",
              "apps/it-security-investment-tool/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/it-security-investment-tool"
          ],
          "options": {
            "jestConfig": "apps/it-security-investment-tool/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "it-security-investment-tool-e2e": {
      "root": "apps/it-security-investment-tool-e2e",
      "sourceRoot": "apps/it-security-investment-tool-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/it-security-investment-tool-e2e/cypress.json",
            "devServerTarget": "it-security-investment-tool:serve:development",
            "tsConfig": "apps/it-security-investment-tool-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "it-security-investment-tool:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/it-security-investment-tool-e2e/**/*.{js,ts}"
            ]
          }
        }
      },
      "tags": [],
      "implicitDependencies": [
        "it-security-investment-tool"
      ]
    }
  }
}
